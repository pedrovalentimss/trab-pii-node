<!DOCTYPE html>
<head>
    <%- include('head'); %>
</head>
<body>
    <header>
        <%- include('menu'); %>
    </header>
    <div class="container mt-3">
        <h2>Quadras cadastradas</h2>
        <table class="table table-hover mt-3">
            <thead class="thead-dark">
                <tr>
                    <th>Nome</th>
                    <th>Telefone</th>
                    <th>Esportes</th>
                    <th>Endere√ßo</th>
                    <th>CEP</th>
                    <th>Cidade</th>
                    <th>Estado</th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
            <% dadosQuadras.forEach(function(dados) { %>
                <tr>
                    <td><%= dados.nome %></td>
                    <td><%= dados.telefone %></td>
                    <td><%= dados.esportes %></td>
                    <td><%= dados.endereco %></td>
                    <td><%= dados.cep %></td>
                    <td><%= dados.cidade %></td>
                    <td><%= dados.estado %></td>
                    <td>
                        <a href="/delete/<%=dados.id %>"> 
                            <span class="material-icons ">delete </span>
                        </a>
                    </td>
                </tr>
                <% }) %>
            </tbody>            
        </table>
    </div>
</body>
</html>